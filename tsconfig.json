{
  "compilerOptions": {
      "outDir": "./target/",
      "sourceMap": true,
      "skipLibCheck": true,
      "noImplicitAny": false,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true
  },
  "include": [
      "./src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}