{
  "name": "primes",
  "version": "1.0.0",
  "description": "technical test",
  "main": "index.js",
  "author": "Luis Miguel Angulo",
  "license": "ISC",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "dev": "webpack -wd",
    "build": "webpack --config webpack.config.js",
    "test": "jest --watch"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest/config/test-shim.js",
      "<rootDir>/jest/config/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jest/config/test-preprocessor.js"
    },
    "testMatch": [
      "**/__test__/*.(ts|tsx|js)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "keywords": [
    "react",
    "typescript",
    "webpack"
  ],
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-number-format": "^4.4.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.9.2",
    "@types/reactstrap": "^8.4.2",
    "@types/redux-mock-store": "^1.0.2",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "node-sass": "^4.14.1",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.4.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0"
  }
}
